<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Version>1.3.0</Version>
    <Authors>David Dvali</Authors>
    <Product>Dvl_Sql.Extensions.Types</Product>
    <PackageId>DvlSql.Extensions.Types</PackageId>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
    <Description>Extension Helpers for DvlSqlType and DvlSqlParameter in DvlSql Package</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/DVLKinoMan/DvlSql.Extensions.Types</PackageProjectUrl>
    <RepositoryUrl>https://github.com/DVLKinoMan/DvlSql.Extensions.Types</RepositoryUrl>
    <PackageTags>DvlSql, DvlSqlType, DvlSqlParameter, Extensions</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DvlSql" Version="1.3.0" />
  </ItemGroup>

</Project>
